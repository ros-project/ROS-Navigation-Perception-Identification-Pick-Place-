<launch>
  <node name="perception_node" pkg="ebot_task4" type="perception_node" output="screen">
    <rosparam>
      cloud_topic: "/camera2/depth/points2"
      world_frame: "base_link"
      camera_frame: "camera_link2"
      voxel_leaf_size: 0.003 <!-- mm -->
      x_filter_min: -2.5 <!-- m -->
      x_filter_max: 2.5 <!-- m -->
      y_filter_min: -2.5 <!-- m -->
      y_filter_max: 2.5 <!-- m -->
      z_filter_min: -2.5 <!-- m -->
      z_filter_max: 2.5 <!-- m -->
      plane_max_iterations: 100
      plane_distance_threshold: 0.05
      cluster_tolerance: 0.01
      cluster_min_size: 250
      cluster_max_size: 500000
      num: 1
    </rosparam>
  </node>


</launch>

